                        /=debug(ds_form.object.name)
                        -display =  ds_form.object.name.present? && (ds_form.object.name.downcase != "won" || ds_form.object.name.downcase != "lost") ? "block" : "none"
                        =hidden_field_tag :none_remove,display
                        .col-md-2{style: "display:block"}
                          %label
