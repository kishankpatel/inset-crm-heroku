<script type="text/javascript">

	function validateDownload() {
		// var name = $.trim($("#name").val());
		var email = $.trim($("#email").val());
		var name = $.trim($("#name").val());
		var gmt_offset = $.trim($("#gmt_offset").val());
		
		var pattern = /^\b[A-Z0-9._%-]+@[A-Z0-9.-]+\.[A-Z]{2,4}\b$/i 
		if(email == "") {
			$("#name").removeClass("error-border");
			$("#email").addClass("error-border");
			$("#email").focus();
			return false;
		} else if(!pattern.test(email)) {
			// $("#name").removeClass("error-border");
			$("#email").addClass("error-border");
			$("#email").focus();
			return false;
		}
		$("#email").removeClass("error-border");
		
		$.ajax({
	      beforeSend: function() {
			//$("#free_download_btn").hide();
			$("#free_download_btn").attr("disabled", "disabled");
			$('#task_loader,.fixed_loader').show();
	      },
	      type: "POST",
	      url: "/download_community_file",
	      data: {name: name, email: email },
	      success: function(data){   
		    if(data == "success"){
		        $('#task_loader,.fixed_loader').hide();
		        $(".free_download_page").hide();
		        $(".thank-you-container").show();
		        $(".inst_supp").show();

		    }else{
		    	$('#task_loader,.fixed_loader').hide();
	        	show_alert_message("danger", "Opps! Something went wrong.");
	        	$("#free_download_btn").removeAttr("disabled");
		    }
	      },
	      error: function(data){
	        $('#task_loader,.fixed_loader').hide();
	        show_alert_message("danger", "Opps! Something went wrong.");
	        $("#free_download_btn").removeAttr("disabled");
	      }
	      
	    });

		$.post(
			"http://blog.wakeupsales.com/auto_subscription_blog.php",
			{email:email,domain:'community'},
