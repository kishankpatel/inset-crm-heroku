.g_started_sec.modal.fade.in
  .gsSlider
    .slide
      .hmodal-success
        .modal-dialog
          .modal-content
            .color-line
            .modal-header
              %h4.modal-title Inset CRM
            .modal-body.text-center
              %h2.font-extra-bold.m-b Welcome
              %h4 Let's get you started
            .modal-footer.text-center
              %a.pager-next.btn.w-xs.btn-success{:href => "javascript:void(0)"} Start
    .slide
      .hmodal-success
        .modal-dialog
          .modal-content
            .color-line
            .modal-header
              %h2.font-bold Your Name
              %h5 Please provide your first and last name
            = form_for(@current_user,:url=> gs_save_name_path, :html=>{:class => "form",:id=>'ga_save_name'},:remote=>true) do  |f|
              .modal-body
                .row
                  .col-md-6
                    .form-group
                      = f.label :first_name
                      = f.text_field  :first_name, :class=>"form-control", :placeholder=>"Enter your first name",:required=>true
                      
                  .col-md-6
                    .form-group
                      = f.label :last_name
                      = f.text_field   :last_name, :class=>"form-control", :placeholder=>"Enter your first name",:required=>true
              .modal-footer
                %button.pager-next.btn.w-xs.btn-success{type: "submit"}
                  %i.icon-chevron-right>
                  Next
    .slide
      .hmodal-success
        .modal-dialog
          .modal-content
            .color-line
            .modal-header
              %h2.font-bold Your Phone Number
            = form_for(@current_user,:url=> gs_save_phone_no_path,:html=> {:class => "form",:id=>'gs_save_phone_no'},:remote=>true) do  |f|
              .modal-body
                .row
                  .col-md-4
                    .form-group
                      - phone = @current_user.phone
                      = f.label :area_code
                      = f.text_field  :area_code, :class=>"form-control", :placeholder=>"Area Code",:required=>true,:value=>phone.present? ? phone.extension : "",:onkeydown => "plusandnumbersonly(event);"
                  .col-md-8
                    .form-group
                      = f.label :phone_no
                      = f.text_field  :phone_no, :class=>"form-control", :placeholder=>"Phone Number",:required=>true,:value=>phone.present? ? phone.phone_no : "", onkeydown: "numbersonly(event);"
              .modal-footer
                %a.pager-prev.btn.w-xs.btn-success.fl{:href => "javascript:void(0)"}
                  %i.icon-chevron-left>
                  Prev
                %button.pager-next.btn.w-xs.btn-success{type: "submit"}
                  %i.icon-chevron-right>
                  Next
                .cb
