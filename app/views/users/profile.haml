        if(data.errors) { //validation error
          $.each(data.errors, function(k, v) { msg += k+": "+v+"<br>"; });
        }
        else if(data.responseText) { //ajax error
        msg = data.responseText;
      }
    }
    });
    $('.profile_email').editable({
