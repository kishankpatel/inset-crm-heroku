
= javascript_include_tag 'jquery.nicescroll.min'
/ = stylesheet_link_tag 'profile'
= hidden_field_tag 'admin_type', @current_user.admin_type

.profile_page
  .normalheader.small-header
    .hpanel
      .panel-body
        #hbreadcrumb.pull-right
          %ol.hbreadcrumb.breadcrumb
            %li.active
              %span Profile
        %h2.font-light.m-b-xs Profile
        %small 
  .content
    .row
      .col-lg-6
        .hpanel.hgreen
          .panel-body
            / .pull-right.text-right
            /   .btn-group
            /     %i.fa.fa-facebook.btn.btn-default.btn-xs
            /     %i.fa.fa-twitter.btn.btn-default.btn-xs
            /     %i.fa.fa-linkedin.btn.btn-default.btn-xs
            .prf_img
              = form_for @user,:url=> user_save_tmp_img_path+"?user_id="+@user.id.to_s, :html=>{:class => "form-horizontal",:id=>"tempImgSaveForm"}, :remote => true, :method => "POST" do |f|
                .fl.img_upload
                  .cms-file-input.ace-file-multiple
                    - if @user.image.present?
                      - image_path = @user.image.image.url(:thumb)
                    -else
                      - image_path = "/assets/new-ui/user.png"
                    #profile_image_div
                      = image_tag image_path, :alt=>"profile image",  :id=>"previewHolder",:class=>"img-circle m-b m-t-md",:onError=>"this.src='/assets/new-ui/user.png';this.onerror=null;"

                  - if @allowed_user
                    .change_pf_image_btn
                      %span.fal.fa-camera
                      Change Image
                    %input{:type=>"hidden",:value=>"#{@user.id}",:id=>"user_id"}
                    =f.file_field "profile_image" ,:class=>"form-control btn-metis-7 btn-file file_hid_block", :onchange=> "checkFile_withtype(this);$('#profileImgModal').modal({backdrop: 'static',show:true});$('#tempImgSaveForm').submit();" 
            /%img.img-circle.m-b.m-t-md{:alt => "logo", :src => "images/profile.jpg"}/
            %h3
              = @user.full_name
              %span.btn.btn-outline.btn-danger2.btn-sm 
                - if @user.admin_type == 1
                  .sup_admin_txt
                    Super Admin
                - elsif @user.admin_type == 2
                  .admin_txt
                    Admin
                - elsif @user.admin_type == 3 && @user.role.present?
                  .user_txt
                    =@user.role.name
                - else
                  .user_txt
                    User
                
            .cb
            .text-muted.font-bold.m-b-xs 
              %span.fal.fa-envelope-square{title:"Email"}
              - begin
                - EmailVerifier.check(@user.email)
                %span{title: @user.email.size > 15 ? @user.email : ""}
                  = @user.email.truncate(15)
              - rescue
                %a.profile_email{href: "#", id: @user.id}
                  = @user.email
            .cb
            .text-muted.font-bold.m-b-xs
              %span.fal.fa-browser{title:"Website"}
              %a.text-success.font-bold.org_website{href: "#", id: @user.id}
                = @user.organization.website
            .cb
            .text-muted.font-bold.m-b-xs 
              %span.fal.fa-phone{title:"Phone"}
              - if @user.phone.present?
                - if @allowed_user
                  %a.profile_phone_number{href: "#", id: @user.id}
                    = (@user.phone.phone_no if @user.phone.present?)
                - else
                  = (@user.phone.phone_no if @user.phone.present?)
              - else
                %a.profile_phone_number{href: "#", id: @user.id}
            .text-muted.font-bold.m-b-xs
              %span.fal.fa-user
              Designation:
              - if @user.is_admin?
                - hash = Hash[@current_organization.user_designations.collect {|c| [ c.id, c.name ]}]
                %a.profile_desg{:href => "#", id: @user.id, "data-name" => @user.id, "data-source" => hash.to_json}
                  = @user.user_designation.present? ? @user.user_designation.name : ""
              - else
                = @user.user_designation.present? ? @user.user_designation.name : ""
            / .progress.m-t-xs.full.progress-small
            /   .progress-bar.progress-bar-success{"aria-valuemax" => "100", "aria-valuemin" => "0", "aria-valuenow" => "65", :role => "progressbar", :style => "width: 65%"}
            /     %span.sr-only 35% Complete (success)
          .panel-body
            %h4.mtop-0 Profile Information
            %p
              %strong
                %span.fal.fa-user
                First Name:
                
              %span
                - if @user.first_name.present?
                  - if @allowed_user
                    %a.profile_first_name{href: "#", id: @user.id}
                      = @user.first_name
                  - else
                    = @user.first_name
                - else
                  %a.profile_first_name{href: "#", id: @user.id}
            %p
              %strong
                %span.fal.fa-user
                Last Name:
              %span
                - if @user.last_name.present?
                  - if @allowed_user
                    %a.profile_last_name{href: "#", id: @user.id}
                      = @user.last_name
                  - else
                    = @user.last_name
                - else
                  %a.profile_last_name{href: "#", id: @user.id}
            %p
              %strong
                %span.fal.fa-key
                Password:
              - if !@user.provider.present? && @user == @current_user
                %a{href: "javascript:", onclick: "$('#user_password').val('');$('#user_password_confirmation').val('');$('#task_alert_msg').hide();$('#changepassModal').modal('show');"} !!!!!!!!!!
              - else
                %div
                  %span.fal.fa-lightbulb{onmouseover:"$('.show_bo_notification').show()", onmouseout:"$('.show_bo_notification').hide()"}
                  .show_bo_notification.fr{style:"position: absolute; display: none; left: 59px; top:-22px;"}
                    .bulb_arrow_left
                    .task_bulb
                      .data-box
                        %div
                          = "You have logged-in using your " + (@user.provider=='linkedin' ? 'LinkedIn' : 'Google') +  " account. Can't change the password. Sorry!"
            %p
              %strong
                %span.fal.fa-university
                Company:
              %span
                =@user.organization.name
            %p
              %strong
                %span.fal.fa-users
                Company size:
              %span
                %font
                  - hash = Hash[CompanyStrength.find(:all).collect {|c| [ c.id, c.range ]}]
                  %a#group.org_size{:href => "#", id: @user.id, "data-name" => @user.id, "data-source" => hash.to_json}
                    = @user.organization.present? && @user.organization.company_strength.present? && @user.organization.company_strength.range.present? ? @user.organization.company_strength.range : ""
            %p
              %strong
                %span.fal.fa-map-marker-alt
                Time Zone:
              - if @allowed_user
                %a.profile_time_zone{:href => "#", :id => @user.id, :"data-value"=> @user.time_zone, style:"font-size: 12px;", title: ActiveSupport::TimeZone["#{@user.time_zone}"].to_s.size > 20 ? ActiveSupport::TimeZone["#{@user.time_zone}"].to_s : ""}
                  = ActiveSupport::TimeZone["#{@user.time_zone}"].to_s.truncate(20)
              - else
                = ActiveSupport::TimeZone["#{@user.time_zone}"]
            %p
              %strong
                %span.fal.fa-clock
                Hourly Billable:
                - user_billable_hr = @user.user_hourly_billable.present? ? @user.user_hourly_billable.amount : ""
                - if @user.is_admin?
                  %a.profile_hrly_billable{:href => "#", id: @user.id, "data-name" => @user.id, "data-value" => user_billable_hr}
                    = user_billable_hr
                - else
                  = user_billable_hr
            - if @current_user.admin_type == 1 || @current_user.admin_type          
              %p
                %strong
                  %span.fab.fa-servicestack
                  Priority Level:
                = @user.priority_label
      .col-lg-6
        - unless @current_user.is_siteadmin?
          
          .prof_det_rt.prof_usr
            #header_user_data

            .cb
        .row
          .hpanel.hgreen
            = hidden_field_tag "user_id", @user.id
            = hidden_field_tag "user_name", @user.full_name
            .panel-body
              #div_deal_user
                =image_tag "/assets/ajax-loader2.gif", style: ""
        

