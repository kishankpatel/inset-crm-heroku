    formValues["gs_save_task_types"]= $('form#gs_save_task_types').serialize();
    formValues["gs_save_task_outcomes"]= $('form#gs_save_task_outcomes').serialize();
    formValues["gs_save_lost_reasons"]= $('form#gs_save_lost_reasons').serialize();
    formValues["gs_save_user_roles"]= $('form#gs_save_user_roles').serialize();
    formValues["user_invite_form"]= $('form#user_invite_form').serialize();
    
    $('a.pager-prev').click(function () {
      var current = slider.getCurrentSlide();
      slider.goToPrevSlide(current) - 1;
    });
    $('a.pager-next').click(function () {
      var current = slider.getCurrentSlide();
      slider.goToNextSlide(current) + 1;
    });
    $('button.pager-next').click(function () {
      var skipvalue = 1;
      if($(this).hasClass("skip2"))
      {
        
        skipvalue = 2
       
        var current = slider.getCurrentSlide();
        slider.goToSlide(current + skipvalue) ;
      }
      else if($(this).closest('form').length > 0)
      {
        if($(this).closest('form').valid())
        {
          formid = $(this).closest('form').attr("id");
          if(formValues[formid] == $('form#' + formid).serialize())
          {
            event.preventDefault();
            var current = slider.getCurrentSlide();
            slider.goToNextSlide(current) ;
            return false;
          }
          else
          {
            var current = slider.getCurrentSlide();
            if($(this).hasClass("add_more")){
              $.post($(this).closest('form').attr("action"), $(this).closest('form').serialize(), function(res){});
              slider.goToSlide(current);
              $(this).closest('form')[0].reset();
              return false;
            }else{
              slider.goToNextSlide(current) + skipvalue;
            }
          }
        }
      }
      else
      {
      var current = slider.getCurrentSlide();
      slider.goToNextSlide(current) + skipvalue;
      }
    });
    
    $("#ga_save_name").bind("ajax:complete",function(){

    })
