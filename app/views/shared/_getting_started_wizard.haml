    // Upload Company Logo
    .slide
      .hmodal-success
        .modal-dialog
          .modal-content
            .color-line
            .modal-header
              %h2.font-bold Upload Your Company Logo
              %h5 Your logo will be used to identify your company in the application and your email templates
            = form_for(@current_user.organization,:url=> gs_save_org_image_path, :html=>{:class => "form",:id=>'gs_save_org_image',:multipart=>true},:remote=>true) do  |f|
              -if !@current_user.organization.image.present?
                - image = Image.new(:organization_id=>@current_user.organization_id)
                - @current_user.organization.image = image
              .modal-body
                .row
                  .col-md-12
                    .form-group.upload_logo
                      
                      /%input{:type=>"hidden",:value=>"#{@user.id}",:id=>"user_id"}
                      =f.fields_for :image do |image_fields|
                        %img{:id=>"org_image_url",:src=>image_fields.object.image.url}
                        =image_fields.label :image,"Upload Image"
                        =image_fields.file_field :image ,:class=>"form-control btn-metis-7 btn-file file_hid_block", :onchange=> "checkFile_withtype(this);$('#gs_save_org_image').submit();" 
                  
              .modal-footer
                %a.pager-prev.btn.w-xs.btn-success.fl{:href => "javascript:void(0)"}
                  %i.icon-chevron-left>
                  Prev
                %a.pager-next.btn.w-xs.btn-success.fr{:href => "javascript:void(0)"}
                  %i.icon-chevron-right>
                  Next
                .cb
    // Industry
    .slide
      .hmodal-success
        .modal-dialog
          .modal-content
            .color-line
            .modal-header
              %h2.font-bold Industry
            = form_for(@current_user.organization,:url=> gs_save_industries_path, :html=>{:class => "form",:id=>'gs_save_industries'},:remote=>true) do  |f|
              .modal-body
                .gs_overflow
                  .row.mr-0
                    - company_industries = CompanyIndustry.all
                    / - if project.new_record?
                    /   - indx = 0
                    /   - all_org_users.each do |u|
                    /     = f.fields_for :project_users do |proj_user|
                    /       %span.col-md-3.checkbox.checkbox-inline
                    /         = proj_user.check_box :user_id, {}, u.id, nil
                    /         %label{for: "project_project_users_attributes_#{indx}_user_id"}
                    /           %span
                    /           = u.full_name.present? ? u.full_name : u.email.split("@")[0]
                    /       - indx = indx + 1
                    / - else
                    - indx = 0
                    - industry_ids = []
                    -org_industries = @current_user.organization.organization_industries.map(&:company_industry_id)
                    - organization_industries = @current_user.organization.organization_industries # if @current_user.organization.organization_industries.count == 0
                    = f.fields_for :organization_industries,:wrapper=>false do |comp_industry|
                      
                      - u = comp_industry.object
                      - if u.present? && u.id.present?
                        - industry_ids << u.id
                        .col-md-6.checkbox.checkbox-inline.checkbox-success
                          = comp_industry.check_box :company_industry_id, {}, u.id, nil
                          %label{for: "organization_organization_industries_attributes_#{indx}_company_industry_id"}
                            %span
                            = u.company_industry.name
                        - indx = indx + 1
                    /=debug(company_industries)
                    - company_industries.each do |u|
                      - unless org_industries.include? u.id
                        .col-md-6.checkbox.checkbox-inline.checkbox-success
                          %input{type: 'checkbox', id: "organization_organization_industries_attributes_#{indx}_company_industry_id", name: "organization[organization_industries_attributes][#{indx}][company_industry_id]", value: u.id}
                          %label{for: "organization_organization_industries_attributes_#{indx}_company_industry_id"}
                            %span
                            = u.name
                        - indx = indx + 1
                
              .modal-footer
                %a.pager-prev.btn.w-xs.btn-success.fl{:href => "javascript:void(0)"}
                  %i.icon-chevron-left>
                  Prev
                %button.pager-next.btn.w-xs.btn-success{type: "submit"}
                  %i.icon-chevron-right>
                  Next
                .cb
