    //Company Address
    .slide
      .hmodal-success
        .modal-dialog
          .modal-content
            .color-line
            .modal-header
              %h2.font-bold Company Address
              %h5 Please enter your organizations primary billing or shipping address
            = form_for(@current_user,:url=> gs_save_address_path, :html=>{:class => "form",:id=>'gs_save_address'},:remote=>true) do  |f|
              -if !@current_user.address.present?
                - address = Address.new(:organization_id=>@current_user.organization_id)
                - @current_user.address = address
              .modal-body
                =f.fields_for :address do |address_fields|
                  .row
                    .col-md-12
                      .form-group
                        = address_fields.label :address,"Street Address"
                        = address_fields.text_field  :address, :class=>"form-control", :placeholder=>"Enter Apt., Suite,Street Address",:required=>true
                  .row
                    .col-md-6
                      .form-group
                        = address_fields.label :city
                        = address_fields.text_field   :city, :class=>"form-control", :placeholder=>"Enter your city",:required=>true
                    .col-md-6
                      .form-group
                        = address_fields.label :state,"State / Province"
                        = address_fields.text_field   :state, :class=>"form-control", :placeholder=>"Enter your state",:required=>true
                  .row
                    .col-md-6
                      .form-group.m-b-none
                        = address_fields.label :country_id , "Country"
                        = address_fields.select :country_id, Country.find(:all).collect {|c| [ c.name, c.id ]}, {},{:class => "form-control js-source-states"}
                    .col-md-6
                      .form-group.m-b-none
                        = address_fields.label :zipcode,"Postal / Zip Code"
                        = address_fields.text_field   :zipcode, :class=>"form-control", :placeholder=>"Enter your zipcode",:required=>true
              .modal-footer
                %a.pager-prev.btn.w-xs.btn-success.fl{:href => "javascript:void(0)"}
                  %i.icon-chevron-left>
                  Prev
                %button.pager-next.btn.w-xs.btn-success{type: "submit"}
                  %i.icon-chevron-right>
                  Next
                .cb
