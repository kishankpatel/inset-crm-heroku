  function display_leads(){
    var user_id = $('#user_id').val();
    var user_name = $('#user_name').val();
    $.cookie("cre_by", "true");
    $.cookie("asg_by", "true");
    $.cookie("cre_by_val", user_id);
    $.cookie("asg_by_val", user_id);
    $.cookie("cre_by_txt", user_name);
    $.cookie("asg_by_txt", user_name);
    window.location.href="/leads"
  }
