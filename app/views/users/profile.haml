        if(data.errors) { //validation error
          $.each(data.errors, function(k, v) { msg += k+": "+v+"<br>"; });
        }
        else if(data.responseText) { //ajax error
        msg = data.responseText;
      }
    }
    });
    $('.org_website').editable({
      url: '/save_profile_info',
      pk: 7,
      title: 'Enter website',
      type: 'text',
      placement: 'top',
      data: {custom: 'text'},
      emptytext: 'Enter website',
      validate: function(value) {
        if($.trim(value) == '' || $.trim(value) == 'NA')
          return 'Please enter website';

      },
      success: function(data) {
        $('.org_website').html(data);
      },
      error: function(data) {
        //alert('error')
        /* actions on validation error (or ajax error) */
        var msg = '';
        if(data.errors) { //validation error
          $.each(data.errors, function(k, v) { msg += k+": "+v+"<br>"; });
        }
        else if(data.responseText) { //ajax error
        msg = data.responseText;
      }
    }
    });
    $('.org_size').editable({
      url: '/save_profile_info',
      pk: 8,
      title: 'Select company size',
      type: 'select',
      value: '#{@user.organization.company_strength.id if @user.organization.company_strength.present?}',
      placement: 'top',
      emptytext: 'Select company size',
