                .row
                  .col-md-12
                    .form-group.m-b-none
                      = f.label :size_id , "Company Size"
                      = f.select :size_id, CompanyStrength.all.uniq(&:range).collect {|c| [ c.range, c.id ]}, {:include_blank => '- Organization Size -'},  :required => "true", :class=>"form-control"
              .modal-footer
                %a.pager-prev.btn.w-xs.btn-success.fl{:href => "javascript:void(0)"}
