      pk: 5,
      title: 'Enter email',
      type: 'text',
      placement: 'top',
      emptytext: 'Enter email',
      data: {custom: 'text'},
      validate: function(value) {
        if($.trim(value) == '' || $.trim(value) == 'NA')
          return 'Please enter email';

      },
      success: function(data) {
        $('.profile_email').html(data);
